{
  "name": "ai-interviewer-backend",
  "version": "1.0.0",
  "description": "AI Interviewer Platform - Backend API and session engine",
  "main": "dist/index.js",
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "scripts": {
    "build": "prisma generate --schema=./prisma/schema.prisma && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "npx prisma db seed",
    "db:schema-scheduled": "node scripts/run-schema-scheduled.cjs"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "node-record-lpcm16": "^1.0.1",
    "nodemailer": "^8.0.1",
    "openai": "^6.22.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.11.3",
    "redis": "^5.11.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "uuid": "^9.0.1",
    "wav": "^1.0.2",
    "winston": "^3.11.0",
    "@prisma/client": "5.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "@types/pg": "^8.11.0",
    "@types/nodemailer": "^6.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.33",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "prisma": "5.17.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
